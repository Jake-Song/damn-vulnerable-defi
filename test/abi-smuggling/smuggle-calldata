# send transaction with calldata encoded manually by example
let funcSig = ethers.utils.id("transfer(address,uint256)").slice(0,10);
let calldata = ethers.utils.hexConcat([
            funcSig, 
            ethers.utils.defaultAbiCoder.encode(['address', 'uint256'], [target2.address, amount])
            ]);
await player.sendTransaction({to: target.address, data: calldata, gasLimit: 1e6});

